<h1>Lists</h1>

  <%= link_to "New List", new_list_path, class: 'btn btn-success' %>
  <%= will_paginate @lists %>


<% @lists.each do |list| %>
    <div class="media">
      <div class="media-body">
        <h4 class="media-heading">
          <%= link_to list.title, list %>
        </h4>
      </div>
    </div>
  <% end %>

