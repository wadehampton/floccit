<h1><%= @list.title %></h1>

<div class="row">
  <div class="col-md-8">
  <%= @list.items %> 

  <%= render partial: 'items/item', locals: { list: @list } %>
  <%= render partial: 'items/form', locals: { list: @list} %>
 </div>



  <%= link_to "Update", edit_list_path(@list), class: 'btn btn-success' %>
  <%= link_to "Delete List", @list, method: :delete, class: 'btn btn-danger', data: { confirm: 'Are you sure you want to delete this list?'} %>

  
 









